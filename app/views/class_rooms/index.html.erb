<h1>Classes</h1>

<% @class_rooms.each do |class_room| %>
  <tr>
    <td><%= class_room.name %></td>
    <% creator = User.find(class_room.user_id) %>
    <td><%= creator.login %></td>
    <td><%= class_room.occupancy %></td>
    <td><%= class_room.price %></td>
    <td><%= class_room.start_date %></td>
    <td><%= class_room.end_date %></td>
    <td><%= class_room.description_id %></td>
    <td><%= class_room.summary %></td>
    <td><%= link_to 'Show', class_room %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Class room', new_class_room_path %>
